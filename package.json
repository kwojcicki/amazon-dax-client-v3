{
  "name": "amazon-dax-client-sdkv3",
  "version": "1.1.0",
  "description": "Amazon DAX Client for JavaScript",
  "license": "Apache-2.0",
  "homepage": "https://github.com/kwojcicki/amazon-dax-client-v3",
  "author": {
    "name": "Krystian Wojcicki"
  },
  "keywords": [
    "aws",
    "dax",
    "dynamodb",
    "cache",
    "amazon"
  ],
  "main": "dist/cjs/AmazonDaxClient.js",
  "module": "dist/esm/AmazonDaxClient.js",
  "types": "src/AmazonDaxClient.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/AmazonDaxClient.js",
      "require": "./dist/cjs/AmazonDaxClient.js",
      "types": "./dist/cjs/AmazonDaxClient.d.ts"
    }
  },
  "files": [
    "dist",
    "src/AmazonDaxClient.d.ts"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json && echo '{\"type\":\"module\"}' > dist/esm/package.json"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.585.0",
    "@aws-sdk/lib-dynamodb": "^3.585.0",
    "@smithy/smithy-client": "^3.0.1",
    "antlr4": "4.8.x",
    "bignumber.js": "^3.0.0",
    "uuid": "^3.3.2"
  },
  "engines": {
    "node": ">= 4.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "typescript": "^5.5.3"
  }
}